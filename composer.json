{
    "name": "heimrichhannot/contao-watchlist-bundle",
    "description": "This utility bundle offers a flexible watchlist for the Contao CMS.",
    "keywords": [
        "contao",
        "watchlist"
    ],
    "type": "contao-bundle",
    "homepage": "https://github.com/heimrichhannot/contao-watchlist-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Heimrich & Hannot",
            "email": "digitales@heimrich-hannot.de",
            "homepage": "http://www.heimrich-hannot.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/heimrichhannot/contao-watchlist-bundle/issues"
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-zip": "*",
        "contao/core-bundle": "^4.4",
        "heimrichhannot/contao-utils-bundle": "^2.199",
        "heimrichhannot/contao-inserttagcollection-bundle": "^1.1",
        "heimrichhannot/contao-request-bundle": "^1.2",
        "symfony/config": "^3.4 || ^4.4",
        "symfony/dependency-injection": "^3.4 || ^4.4",
        "symfony/http-foundation": "^3.4 || ^4.4",
        "symfony/http-kernel": "^3.4 || ^4.4",
        "symfony/routing": "^3.4 || ^4.4"
    },
    "require-dev": {
        "contao/test-case": "1.1.*",
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.2",
        "phpunit/phpunit": "^6.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "php-coveralls/php-coveralls": "^2.0",
        "symfony/phpunit-bridge": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "HeimrichHannot\\WatchlistBundle\\": "src/",
            "Contao\\": "src/Contao/"
        },
        "classmap": [
            "src/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "contao-manager-plugin": "HeimrichHannot\\WatchlistBundle\\ContaoManager\\Plugin"
    }
}
