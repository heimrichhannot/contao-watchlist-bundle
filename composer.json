{
    "name": "heimrichhannot/contao-watchlist-bundle",
    "description": "This utility bundle offers a flexible watchlist for the Contao CMS.",
    "keywords": [
        "contao",
        "watchlist"
    ],
    "type": "contao-bundle",
    "homepage": "https://github.com/heimrichhannot/contao-watchlist-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Heimrich & Hannot",
            "email": "digitales@heimrich-hannot.de",
            "homepage": "http://www.heimrich-hannot.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/heimrichhannot/contao-watchlist-bundle/issues"
    },
    "require": {
        "php": "^8.1",
        "ext-zip": "*",
        "ext-json": "*",
        "contao/core-bundle": "^4.13 || ^5.0",
        "doctrine/dbal": " ^2.10 || ^3.0",
        "heimrichhannot/contao-encore-contracts": "^1.0",
        "heimrichhannot/contao-utils-bundle": "^2.244 || ^3.9",
        "symfony/config": "^5.4 || ^6.0 || ^7.0",
        "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
        "symfony/event-dispatcher-contracts": "^1.0 || ^2.0 || ^3.0",
        "symfony/http-foundation": "^5.4 || ^6.0 || ^7.0",
        "symfony/http-kernel": "^5.4 || ^6.0 || ^7.0",
        "symfony/routing": "^5.4 || ^6.0 || ^7.0",
        "symfony/translation-contracts": "^1.0 || ^2.0 || ^3.0"
    },
    "old": {
        "heimrichhannot/contao-inserttagcollection-bundle": "^1.1",
        "heimrichhannot/contao-twig-support-bundle": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "HeimrichHannot\\WatchlistBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "HeimrichHannot\\WatchlistBundle\\ContaoManager\\Plugin"
    }
}
